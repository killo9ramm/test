<UserControl x:Class="RBClient.WPF.UserControls.IteremWpfControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40*" />
            <RowDefinition Height="260*" />
        </Grid.RowDefinitions>
        <TextBox VerticalContentAlignment="Center" FontSize="13" HorizontalAlignment="Stretch" Margin="2" Name="textBox" VerticalAlignment="Stretch" TextChanged="textBox1_TextChanged" />
        <ListBox Grid.Row="1" HorizontalAlignment="Stretch" FontSize="13" Margin="3" Name="listBox" VerticalAlignment="Stretch"/>
    </Grid>
</UserControl>
